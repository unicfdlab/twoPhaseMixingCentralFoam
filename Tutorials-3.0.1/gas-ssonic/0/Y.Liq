FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Y.Liq;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    sides1
    {
        type            zeroGradient;
    }
    sides2
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}

