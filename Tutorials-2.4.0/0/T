FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 55.115;

boundaryField
{
    inlet
    {
        type            totalTemperature;
        phi             phi;
        psi             thermo:psi;
        gamma           0.99;
        T0              uniform 55.556;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    #include "constrain.include"
}

