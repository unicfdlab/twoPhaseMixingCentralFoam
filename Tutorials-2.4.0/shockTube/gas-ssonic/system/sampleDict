FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       csv;

sets
(
    line
    {
        type    face;
        axis    x;
        start   ( -0.152 0 0 );
        end     (  0.152 0 0 );
        nPoints 100;
    }
);

fields          ( p U T );


