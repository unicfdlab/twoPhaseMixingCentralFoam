FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     twoPhaseMixingCentralFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         2.5e-4;

deltaT          1e-07;//2e-05;

writeControl    adjustableRunTime;

writeInterval   0.5e-4;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  false;

maxCo           0.2;

maxDeltaT       1;

CourantType	faceCourant;

