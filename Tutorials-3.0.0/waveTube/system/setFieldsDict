FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue Y.Liq 1
);

regions
(
    boxToCell
    {
        box (-1 -1 -1) (5 1 1);
        fieldValues
        (
            volScalarFieldValue Y.Liq 0

        );
    }

);


